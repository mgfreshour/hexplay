{
  "env": {
    "node": true,
    "jasmine": true,
    "es6": true
  },
  "rules": {
    "brace-style": [2, "1tbs"],
    "default-case": 2,
    "func-style": 0,
    "guard-for-in": 2,
    "no-floating-decimal": 2,
    "no-nested-ternary": 2,
    "no-undefined": 2,
    "radix": 2,
    "space-after-keywords": [2, "always"],
    "no-multi-spaces": 0,
    "wrap-iife": 2,
    "quotes": [1, "single"],
    "strict": [1, "global"],
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "camelcase": 0
  },
  "plugins": [
  ]
}
